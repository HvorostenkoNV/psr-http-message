{
  "name": "hnv/psr-http-message",
  "description": "PSR-7 message implementation package: requests and responses",
  "keywords": [
    "psr-7",
    "message",
    "request",
    "response"
  ],
  "authors": [
    {
      "name": "Nikita Hvorostenko",
      "email": "hvorostenko.nikita@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "hnv/psr-http-interfaces": ">=1.0",
    "hnv/psr-http-uri": ">=1.0",
    "hnv/psr-http-stream": ">=1.0",
    "hnv/psr-http-uploaded-file": ">=1.0"
  },
  "require-dev": {
    "phpunit/phpunit": ">=9.0"
  },
  "autoload": {
    "psr-4": {
      "HNV\\Http\\Message\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "HNV\\Http\\MessageTests\\": "tests/"
    }
  }
